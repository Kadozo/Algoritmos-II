<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="267.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CarregarMapaController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="309.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
      <children>
            <Label fx:id="NomeDoArquivoLabel" layoutX="23.0" layoutY="72.0" prefHeight="25.0" prefWidth="138.0" text="Nome do Arquivo:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Button fx:id="CarregarMapaButtom" layoutX="441.0" layoutY="128.0" mnemonicParsing="false" onAction="#clicouCarregarBotao" prefHeight="50.0" prefWidth="103.0" text="Carregar" />
            <TextField fx:id="DigitarArquivo" layoutX="177.0" layoutY="68.0" onAction="#digitouCarregar" prefHeight="25.0" prefWidth="373.0" />
            <Button fx:id="VoltarCarregarButtom" layoutX="528.0" layoutY="200.0" mnemonicParsing="false" onAction="#clicouVoltarBotaoMapa" prefHeight="42.0" prefWidth="65.0" text="Voltar" />
            <Label fx:id="CarregarMapaLabel" alignment="CENTER" layoutX="19.0" layoutY="14.0" prefHeight="17.0" prefWidth="541.0" text="Carregar Mapa">
               <font>
                  <Font name="Times New Roman" size="29.0" />
               </font>
            </Label>
            <Label fx:id="labelConfirmacao" alignment="CENTER" disable="true" layoutX="21.0" layoutY="125.0" prefHeight="50.0" prefWidth="394.0" text="Nenhum Arquivo Carregado">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
